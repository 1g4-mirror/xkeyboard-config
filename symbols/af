partial alphanumeric_keys
xkb_symbols "ps-olpc" {

   name[Group1]= "Pashto (Afghanistan, OLPC)";

   key <TLDE> { [ U200D, U0654, grave ] }; // zero width joiner, Arabic hamza above
   key <AE01> { [ U06F1, exclam, asciitilde ] }; // Arabic one
   key <AE02> { [ U06F2, U066C, at ] }; // Arabic two, Arabic thousands separator
   key <AE03> { [ U06F3, U066B, numbersign ] }; // Arabic three, Arabic decimal separator
   key <AE04> { [ U06F4, U060B, dollar ] }; // Arabic four, Afghani sign
   key <AE05> { [ U06F5, U066A, percent ] }; // Arabic five, Arabic percent sign
   key <AE06> { [ U06F6, multiply, asciicircum ] }; // Arabic six
   key <AE07> { [ U06F7, guillemotright, ampersand ] }; // Arabic seven
   key <AE08> { [ U06F8, guillemotleft, U066D ] }; // Arabic eight, Arabic five-pointed star
   key <AE09> { [ U06F9, parenright, enfilledcircbullet ] }; // Arabic nine
   key <AE10> { [ U06F0, parenleft, degree ] }; // Arabic zero
   key <AE11> { [ minus, U0640, underscore ] }; // Arabic tatweel
   key <AE12> { [ equal, plus, division ] };

   key <AD01> { [ U0636, U0652, EuroSign ] }; // Arabic dad, Arabic sukun
   key <AD02> { [ U0635, U064C, U0671 ] }; // Arabic sad, Arabic dammatan, Arabic alef walsa
   key <AD03> { [ U062B, U064D, U0649 ] }; // Arabic theh, Arabic kasratan, Arabic alef maksura initial form
   key <AD04> { [ U0642, U064B, U200E ] }; // Arabic qaf, Arabic fathatan, left-to-right mark
   key <AD05> { [ U0641, U064F, U200F ] }; // Arabic feh, Arabic damma, right-to-left mark
   key <AD06> { [ U063A, U0650, UE653 ] }; // Arabic ghain, Arabic kasra, Arabic alef with madda above
   key <AD07> { [ U0639, U064E, UE659 ] }; // Arabic ain, Arabic fatha, Arabic zwarakay
   key <AD08> { [ U0647, U0651, U0670 ] }; // Arabic heh, Arabic shadda, Arabic superscript alef
   key <AD09> { [ U062E, U0681, apostrophe ] }; // Arabic khah, Arabic hah with hamza above
   key <AD10> { [ U062D, U0685, quotedbl ] }; // Arabic hah, Arabic hah with three dots above
   key <AD11> { [ U062C, bracketright, braceleft ] }; // Arabic jeem
   key <AD12> { [ U0686, bracketleft, braceright ] }; // Arabic tcheh

   key <AC01> { [ U0634, U069A ] }; // Arabic sheen, Arabic seen with dot below and dot above
   key <AC02> { [ U0633, U06CD ] }; // Arabic seen, Arabic yeh with tail
   key <AC03> { [ U06CC, U064A, U06D2 ] }; // Farsi yeh, Arabic yeh, Arabic yeh barree
   key <AC04> { [ U0628, U067E, U06BA ] }; // Arabic beh, Arabic peh, Arabic noon ghunna
   key <AC05> { [ U0644, U0623, U06B7 ] }; // Arabic lam, Arabic hamza on alef, Arabic alef with hamza above
   key <AC06> { [ U0627, U0622, U0625 ] }; // Arabic alef, Arabic madda on alef, Arabic alef with hamza below
   key <AC07> { [ U062A, U067C, U0679 ] }; // Arabic teh, Arabic teh with ring, Arabic tteh
   key <AC08> { [ U0646, U06BC, greater ] }; // Arabic noon, Arabic noon with ring
   key <AC09> { [ U0645, U0629, less ] }; // Arabic meem, Arabic teh marbuta
   key <AC10> { [ U06A9, colon, U0643 ] }; // Arabic keheh, Arabic kaf
   key <AC11> { [ U06AB, U061B, U06AF ] }; // Arabic kaf with ring, Arabic semicolon, Arabic gaf

   key <BKSL> { [ backslash, asterisk, bar ] };

   key <AB01> { [ U0638, U0626, question] }; // Arabic zah, Arabic yeh with hamza above
   key <AB02> { [ U06D0, U0637, semicolon ] }; // Arabic tah, Arabic E
   key <AB03> { [ U0632, U0698 ] }; // Arabic zain, Arabic jeh
   key <AB04> { [ U0631, U0621 ] }; // Arabic reh, Arabic hamza
   key <AB05> { [ U0630, U200C ] }; // Arabic thal, zero width non-joiner
   key <AB06> { [ U062F, U0689, U0688 ] }; // Arabic dal, Arabic dal with ring, Arabic ddal
   key <AB07> { [ U0693, U0624, U0691 ] }; // Arabic reh with ring, Arabic waw with hamza above, Arabic rreh
   key <AB08> { [ U0648, period, comma ] }; // Arabic waw, Arabic comma
   key <AB09> { [ U0696, U002E, U06C7 ] }; // Arabic reh with dot below and dot above, full stop, Arabic letter U
   key <AB10> { [ slash, U061F, U06C9 ] }; // Arabic question mark, Arabic kirghiz yu

   include "nbsp(zwnj2nb3)"
   include "group(olpc)"
};

partial alphanumeric_keys
xkb_symbols "fa-olpc" {

   name[Group1]= "Dari (Afghanistan, OLPC)";

   key <TLDE> { [ U200D, division, asciitilde ] }; // zero width joiner
   key <AE01> { [ U06F1, exclam, grave ] }; // Arabic one
   key <AE02> { [ U06F2, U066C, at ] }; // Arabic two, Arabic thousands separator
   key <AE03> { [ U06F3, U066B, numbersign ] }; // Arabic three, Arabic decimal separator
   key <AE04> { [ U06F4, U060B, dollar ] }; // Arabic four, Afghani sign
   key <AE05> { [ U06F5, U066A, percent ] }; // Arabic five, Arabic percent sign
   key <AE06> { [ U06F6, multiply, asciicircum ] }; // Arabic six
   key <AE07> { [ U06F7, U060C, ampersand ] }; // Arabic seven, Arabic comma
   key <AE08> { [ U06F8, asterisk, enfilledcircbullet ] }; // Arabic eight
   key <AE09> { [ U06F9, parenright, U200E ] }; // Arabic nine, left-to-right mark
   key <AE10> { [ U06F0, parenleft, U200F ] }; // Arabic zero, right-to-left mark
   key <AE11> { [ minus, U0640, underscore ] }; // Arabic tatweel
   key <AE12> { [ equal, plus ] };

   key <AD01> { [ U0636, U0652, degree ] }; // Arabic dad, Arabic sukun
   key <AD02> { [ U0635, U064C ] }; // Arabic sad, Arabic dammatan
   key <AD03> { [ U062B, U064D, EuroSign ] }; // Arabic theh, Arabic kasratan
   key <AD04> { [ U0642, U064B, UFD3E ] }; // Arabic qaf, Arabic fathatan, ornate left paren
   key <AD05> { [ U0641, U064F, UFD3F ] }; // Arabic feh, Arabic damma, ornate right paren
   key <AD06> { [ U063A, U0650, U0656 ] }; // Arabic ghain, Arabic kasra, Arabic subscript alef
   key <AD07> { [ U0639, U064E, UE659 ] }; // Arabic ain, Arabic fatha, Arabic zwarakay
   key <AD08> { [ U0647, U0651, U0655 ] }; // Arabic heh, Arabic shadda, Arabic hamza below
   key <AD09> { [ U062E, bracketright, apostrophe ] }; // Arabic khah
   key <AD10> { [ U062D, bracketleft, quotedbl ] }; // Arabic hah
   key <AD11> { [ U062C, braceright, U0681 ] }; // Arabic jeem, Arabic hah with hamza above
   key <AD12> { [ U0686, braceleft, U0685 ] }; // Arabic tcheh, Arabic hah with three dots above

   key <AC01> { [ U0634, U0624, U069A ] }; // Arabic sheen, Arabic waw with hamza above, Arabic seen with dot below and dot above
   key <AC02> { [ U0633, U0626, U06CD ] }; // Arabic seen, Arabic yeh with hamza above, Arabic yeh with tail
   key <AC03> { [ U06CC, U064A, U0649 ] }; // Farsi yeh, Arabic yeh, Arabic alef maksura
   key <AC04> { [ U0628, U0625, U06D0 ] }; // Arabic beh, Arabic alef with hamza below, Arabic e
   key <AC05> { [ U0644, U0623, U06B7 ] }; // Arabic lam, Arabic hamza on alef, Arabic alef with hamza above
   key <AC06> { [ U0627, U0622, U0671 ] }; // Arabic alef, Arabic madda on alef, Arabic alef wasla
   key <AC07> { [ U062A, U0629, U067C ] }; // Arabic teh, Arabic teh marbuta, Arabic tteh
   key <AC08> { [ U0646, guillemotright, U06BC ] }; // Arabic noon, Arabic noon with ring
   key <AC09> { [ U0645, guillemotleft, U06BA ] }; // Arabic meem, Arabic noon ghunna
   key <AC10> { [ U06A9, colon, semicolon ] }; // Arabic keheh
   key <AC11> { [ U06AF, U061B, U06AB ] }; // Arabic gaf, Arabic semicolon, Arabic kaf with ring

   key <BKSL> { [ backslash, bar, question ] };

   key <AB01> { [ U0638, U0643, U06D2 ] }; // Arabic zah, Arabic kaf, Arabic yeh barree
   key <AB02> { [ U0637, U0653, U0691 ] }; // Arabic tah, Arabic maddah above, Arabic rreh
   key <AB03> { [ U0632, U0698, U0696 ] }; // Arabic zain, Arabic jeh,  Arabic reh with dot below and dot above
   key <AB04> { [ U0631, U0670, U0693 ] }; // Arabic reh, Arabic superscript alef, Arabic reh with ring
   key <AB05> { [ U0630, U200C, U0688 ] }; // Arabic thal, zero width non-joiner, Arabic ddal
   key <AB06> { [ U062F, U0654, U0689 ] }; // Arabic dal, Arabic hamza above, Arabic dal with ring
   key <AB07> { [ U067E, U0621, U0679 ] }; // Arabic peh, Arabic hamza, Arabic tteh
   key <AB08> { [ U0648, greater, comma ] }; // Arabic waw
   key <AB09> { [ period, less, U06C7 ] }; // Arabic u
   key <AB10> { [ slash, U061F, U06C9 ] }; // Arabic question mark, Arabic kirghiz yu

   include "nbsp(zwnj2nb3)"
   include "group(olpc)"
};

