xkb_symbols "uz-olpc" {

   name[Group1]= "Uzbek (Afghanistan, OLPC)";

   key <TLDE> { [ U200D, division, asciitilde ] }; // zero width joiner
   key <AE01> { [ U06F1, exclam, grave ] }; // Arabic one
   key <AE02> { [ U06F2, U066C, at ] }; // Arabic two, Arabic thousands separator
   key <AE03> { [ U06F3, U066B, numbersign ] }; // Arabic three, Arabic decimal separator
   key <AE04> { [ U06F4, U060B, dollar ] }; // Arabic four, Afghani sign
   key <AE05> { [ U06F5, U066A, percent ] }; // Arabic five, Arabic percent sign
   key <AE06> { [ U06F6, multiply, asciicircum ] }; // Arabic six
   key <AE07> { [ U06F7, U060C, ampersand ] }; // Arabic seven, Arabic comma
   key <AE08> { [ U06F8, asterisk, enfilledcircbullet ] }; // Arabic eight
   key <AE09> { [ U06F9, parenright, U200E ] }; // Arabic nine, left-to-right mark
   key <AE10> { [ U06F0, parenleft, U200F ] }; // Arabic zero, right-to-left mark
   key <AE11> { [ minus, U0640, underscore ] }; // Arabic tatweel
   key <AE12> { [ equal, plus ] };

   key <AD01> { [ U0636, U0652, degree ] }; // Arabic dad, Arabic sukun
   key <AD02> { [ U0635, U064C, U0653 ] }; // Arabic sad, Arabic dammatan, Arabic maddah above
   key <AD03> { [ U062B, U064D, EuroSign ] }; // Arabic theh, Arabic kasratan
   key <AD04> { [ U0642, U064B, UFD3E ] }; // Arabic qaf, Arabic fathatan, ornate left paren
   key <AD05> { [ U0641, U064F, UFD3F ] }; // Arabic feh, Arabic damma, ornate right paren
   key <AD06> { [ U063A, U0650, U0656 ] }; // Arabic ghain, Arabic kasra, Arabic subscript alef
   key <AD07> { [ U0639, U064E, UE659 ] }; // Arabic ain, Arabic fatha, Arabic zwarakay
   key <AD08> { [ U0647, U0651, U0670 ] }; // Arabic heh, Arabic shadda, Arabic superscript alef
   key <AD09> { [ U062E, bracketright, apostrophe ] }; // Arabic khah
   key <AD10> { [ U062D, bracketleft, quotedbl ] }; // Arabic hah
   key <AD11> { [ U062C, braceright, U0681 ] }; // Arabic jeem, Arabic hah with hamza above
   key <AD12> { [ U0686, braceleft, U0685 ] }; // Arabic tcheh, Arabic hah with three dots above

   key <AC01> { [ U0634, U0624, U069A ] }; // Arabic sheen, Arabic waw with hamza above, Arabic seen with dot below and dot above
   key <AC02> { [ U0633, U0626, U06CD ] }; // Arabic seen, Arabic yeh with hamza above, Arabic yeh with tail
   key <AC03> { [ U06CC, U064A, U0649 ] }; // Farsi yeh, Arabic yeh, Arabic alef maksura
   key <AC04> { [ U0628, U06D0, U0643  ] }; // Arabic beh, Arabic e, Arabic kaf
   key <AC05> { [ U0644, U0623, U06B7 ] }; // Arabic lam, Arabic hamza on alef, Arabic alef with hamza above
   key <AC06> { [ U0627, U0622, U0671 ] }; // Arabic alef, Arabic madda on alef, Arabic alef wasla
   key <AC07> { [ U062A, U0629, U067C ] }; // Arabic teh, Arabic teh marbuta, Arabic tteh
   key <AC08> { [ U0646, guillemotright, U06BC ] }; // Arabic noon, Arabic noon with ring
   key <AC09> { [ U0645, guillemotleft, U06BA ] }; // Arabic meem, Arabic noon ghunna
   key <AC10> { [ U06A9, colon, semicolon ] }; // Arabic keheh
   key <AC11> { [ U06AF, U061B, U06AB ] }; // Arabic gaf, Arabic semicolon, Arabic kaf with ring

   key <BKSL> { [ backslash, bar, question ] };

   key <AB01> { [ U0638, U06C9, U06D2 ] }; // Arabic zah, Arabic kirghiz yu, Arabic yeh barree
   key <AB02> { [ U0637, U06C7, U0691 ] }; // Arabic tah, Arabic u, Arabic rreh
