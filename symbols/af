// Keyboard layouts for Afghanistan.

// Layouts for Dari, Pashto, and Southern Uzbek.
// Original author: M. Emal Alekozai <memala@gmx.net>, 2006.
//
// Based on the "Computer Locale Requirements for Afghanistan" [1]
// from the "United Nations Development Programme for Afghanistan" and the
// "Afghan Transitional Islamic Administration Ministry of Communications".
// [1] https://www.evertype.com/standards/af/

default partial alphanumeric_keys
xkb_symbols "basic" {
   name[Group1]= "Dari";

   key <TLDE> { [ U200D, U00F7, dead_tilde ] };
   key <AE01> { [ U06F1, exclam, U0060 ] };
   key <AE02> { [ U06F2, U066C, U0040 ] };
   key <AE03> { [ U06F3, U066B, numbersign ] };
   key <AE04> { [ U06F4, UE60B, U0024] };
   key <AE05> { [ U06F5, U066A, U0025 ] };
   key <AE06> { [ U06F6, multiply, U005E ] };
   key <AE07> { [ U06F7, Arabic_comma, U0026 ] };
   key <AE08> { [ U06F8, asterisk, U2022 ] };
   key <AE09> { [ U06F9, U0029, U200E ] };
   key <AE10> { [ U06F0, U0028, U200F ] };
   key <AE11> { [ minus, Arabic_tatweel, U005F ] };
   key <AE12> { [ equal, plus ] };

   key <AD01> { [ Arabic_dad, Arabic_sukun, U00B0 ] };
   key <AD02> { [ Arabic_sad, Arabic_dammatan ] };
   key <AD03> { [ Arabic_theh, Arabic_kasratan, U20AC ] };
   key <AD04> { [ Arabic_qaf, Arabic_fathatan, UFD3E ] };
   key <AD05> { [ Arabic_feh, Arabic_damma, UFD3F ] };
   key <AD06> { [ Arabic_ghain, Arabic_kasra, UE656] };
   key <AD07> { [ Arabic_ain, Arabic_fatha, UE659] };
   key <AD08> { [ Arabic_heh, Arabic_shadda, U0655] };
   key <AD09> { [ Arabic_khah, bracketright, U0027] };
   key <AD10> { [ Arabic_hah, bracketleft, U0022] };
   key <AD11> { [ Arabic_jeem, braceright, U0681 ] };
   key <AD12> { [ U0686, braceleft, U0685 ] };

   key <AC01> { [ Arabic_sheen, Arabic_hamzaonwaw, U069A ] };
   key <AC02> { [ Arabic_seen, Arabic_hamzaonyeh, U06CD ] };
   key <AC03> { [ U06CC, Arabic_yeh, U0649 ] };
   key <AC04> { [ Arabic_beh, Arabic_hamzaunderalef, U06D0 ] };
   key <AC05> { [ Arabic_lam, Arabic_hamzaonalef, U06B7 ] };
   key <AC06> { [ Arabic_alef, Arabic_maddaonalef, U0671 ] };
   key <AC07> { [ Arabic_teh, Arabic_tehmarbuta, U067C ] };
   key <AC08> { [ Arabic_noon, U00BB, U06BC ] };
   key <AC09> { [ Arabic_meem, U00AB, U06BA ] };
   key <AC10> { [ U06A9, colon, U003B ] };
   key <AC11> { [ U06AF, Arabic_semicolon, U06AB ] };

   key <BKSL> { [ backslash, bar, U003F ] };

   key <AB01> { [ Arabic_zah, Arabic_kaf, U06D2] };
   key <AB02> { [ Arabic_tah, U0653 , U0691 ] };
   key <AB03> { [ Arabic_zain, U0698, U0696 ] };
   key <AB04> { [ Arabic_ra, U0670 , U0693 ] };
   key <AB05> { [ Arabic_thal, U200C, U0688  ] };
   key <AB06> { [ Arabic_dal, U0654, U0689 ] };
   key <AB07> { [ U067E, Arabic_hamza, U0679 ] };
   key <AB08> { [ Arabic_waw, greater, U002C ] };
   key <AB09> { [ period, less, U06C7 ] };
   key <AB10> { [ slash, Arabic_question_mark, U06C9 ] };

   include "nbsp(zwnj2nb3)"
   include "level3(ralt_switch)"
};

